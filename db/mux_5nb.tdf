--synthesis_resources = lut 16 
SUBDESIGN mux_5nb
( 
	data[23..0]	:	input;
	result[7..0]	:	output;
	sel[1..0]	:	input;
) 
VARIABLE 
	data0_wire[7..0]	: WIRE;
	data1_wire[7..0]	: WIRE;
	data2_wire[7..0]	: WIRE;
	result_node[7..0]	: WIRE;

BEGIN 
	data0_wire[] = (data[7..0] & (! sel[0..0]));
	data1_wire[] = (data[15..8] & sel[0..0]);
	data2_wire[] = (data[23..16] & sel[1..1]);
	result[] = result_node[];
	result_node[] = (((data0_wire[] # data1_wire[]) & (! sel[1..1])) # data2_wire[]);
END;
